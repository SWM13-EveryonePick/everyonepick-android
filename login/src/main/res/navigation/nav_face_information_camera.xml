<?xml version="1.0" encoding="utf-8"?>

<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_camera"
    app:startDestination="@id/fragmentParentPermission">

    <fragment
        android:id="@+id/fragmentParentPermission"
        android:name="org.soma.everyonepick.login.ui.faceinformation.camerafragments.ParentPermissionFragment"
        android:label="ParentPermissionFragment">

        <action
            android:id="@+id/to_face_information_preview"
            app:destination="@id/fragmentFaceInformationPreview"
            app:popUpTo="@id/fragmentParentPermission"
            app:popUpToInclusive="true" />

    </fragment>

    <fragment
        android:id="@+id/fragmentFaceInformationPreview"
        android:name="org.soma.everyonepick.login.ui.faceinformation.camerafragments.FaceInformationPreviewFragment"
        android:label="FaceInformationPreviewFragment"/>

</navigation>